@model List<PW3.Entidades.UsuarioEntidade>

<h1> Lista de Usuários</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">id</th>
            <th scope="col">email</th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var usuario in Model)
        {
            <tr>
                <th>@usuario.Id</th>
                <td>@usuario.Email</td>
                <td></td>
            </tr>
        }
    </tbody>
</table>